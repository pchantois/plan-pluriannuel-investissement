<html>
	<head>
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('themes/html5up-helios/assets/css/main.css')}}" />
            <link rel="stylesheet" href="{{ asset('assets/css/site.css')}}" />
            <link rel="stylesheet" href="{{ asset('themes/base/css/site.css')}}" />
			<noscript><link rel="stylesheet" href="{{ asset('assets/css/noscript.css')}}" /></noscript>
        {% endblock %}
	</head>
        {# different style for page is : homepage, right-sidebar, left-sidebar, no-sidebar #}
        {% if pageStyle is defined %}
        <body class="{{pageStyle}}">
            <div class="{{pageStyle}} is-preload">
        {% else %}
        <body class="homepage">
            <div class="homepage is-preload">
        {% endif %}

        <div id="page-wrapper">

            <!-- Header -->
                <div id="header">

                    <!-- Inner -->
                        <div class="inner">
                            <header>
                                <h1><a href="index.html" id="logo">Helios</a></h1>
                            </header>
                            {% if pageStyle is not defined or pageStyle == 'homepage' %}
                            <footer>
                                <a href="#banner" class="button circled scrolly">Start</a>
                            </footer>
                            {% endif %}
                        </div>

                        {{ render(controller('App\\Controller\\Objet\\MenuController::index')) }}

                </div>

        {% block body %}{% endblock %}


            <!-- Footer -->
                <div id="footer">
                    <div class="container">
                        <div class="row">

                            <!-- Tweets -->
                                <section class="col-4 col-12-mobile">
                                    <header>
                                        <h2 class="icon fa-twitter circled"><span class="label">Tweets</span></h2>
                                    </header>
                                    <ul class="divided">
                                        <li>
                                            <article class="tweet">
                                                Amet nullam fringilla nibh nulla convallis tique ante sociis accumsan.
                                                <span class="timestamp">5 minutes ago</span>
                                            </article>
                                        </li>
                                        <li>
                                            <article class="tweet">
                                                Hendrerit rutrum quisque.
                                                <span class="timestamp">30 minutes ago</span>
