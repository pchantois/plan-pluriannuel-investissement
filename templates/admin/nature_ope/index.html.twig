{% extends 'base_' ~ ga_theme ~ '.html.twig' %}

{% block title %}NatureOpe index{% endblock %}

{% block body %}
    <h1>NatureOpe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nature_ope in nature_opes %}
            <tr>
                <td>{{ nature_ope.id }}</td>
                <td>{{ nature_ope.libelle }}</td>
                <td>
                    <a href="{{ path('admin_nature_ope_show', {'id': nature_ope.id}) }}">show</a>
                    <a href="{{ path('admin_nature_ope_edit', {'id': nature_ope.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_nature_ope_new') }}">Create new</a>
{% endblock %}
