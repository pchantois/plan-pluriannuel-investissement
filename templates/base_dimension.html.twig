<html>
	<head>
        <title>{% block title %}Welcome!{% endblock %}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        
		{% block stylesheets %}
			<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
			<link rel="stylesheet" href="{{ asset('themes/dimension/assets/css/main.css')}}" />
            <link rel="stylesheet" href="{{ asset('assets/css/site.css')}}" />
            <link rel="stylesheet" href="{{ asset('themes/base/css/site.css')}}" />
			<noscript><link rel="stylesheet" href="{{ asset('themes/dimension/assets/css/noscript.css')}}" /></noscript>
        {% endblock %}
	</head>
	<body class="is-preload">

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Header -->
					<header id="header">
						<div class="logo">
							<span class="icon fa-diamond"></span>
						</div>
						<div class="content">
							<div class="inner">
								<h1>Dimension</h1>
								<p>A fully responsive site template designed by <a href="https://html5up.net">HTML5 UP</a> and released<br />
								for free under the <a href="https://html5up.net/license">Creative Commons</a> license.</p>
							</div>
						</div>
						{{ render(controller('App\\Controller\\Objet\\MenuController::index')) }}
					</header>

					<!-- Main -->
						<div id="main">
        					{% block body %}{% endblock %}
						</div>

				<!-- Footer -->
                <footer id="footer">
                    <p class="copyright">&copy; Untitled. Design: <a href="https://html5up.net">HTML5 UP</a>.</p>
                </footer>

        </div>

    <!-- BG -->
        <div id="bg"></div>

        {% block javascripts %}
		<!-- Scripts -->
			<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
			<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
            <script src="{{asset('themes/dimension/assets/js/browser.min.js')}}"></script>
            <script src="{{asset('themes/dimension/assets/js/breakpoints.min.js')}}"></script>
            <script src="{{asset('themes/dimension/assets/js/util.js')}}"></script>
            <script src="{{asset('themes/dimension/assets/js/main.js')}}"></script>
        {% endblock %}
	</body>
</html>
